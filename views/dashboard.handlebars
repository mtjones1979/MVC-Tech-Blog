{{#if posts.length}}
<section>
    <h2>Your Posts:</h2>
        {{#each posts as |post|}}
            {{> dashPostInfo post}}
        {{/each}}
<button type="submit" class="btn"><a class="link" href="/dashboard">New Post</a></button>
</section>
{{/if}}



<script src="/js/newPost.js"></script>